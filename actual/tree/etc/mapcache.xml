<mapcache>
    <metadata>
     <title>map service</title>
     <abstract>Contains various cached maps</abstract>
    </metadata>

    <cache name="disk" type="disk">
     <base>/tmp/</base>
    </cache>

   <source name="admin_countries" type="wms">
     <getmap>
       <params>
         <FORMAT>image/JPEG</FORMAT>
         <LAYERS>ADMIN</LAYERS>
         <MAP>/etc/geo/wms.map</MAP>
       </params>
     </getmap>
     <http>
       <url>http://localhost/map/?</url>
     </http>
    </source>

   <source name="zelda" type="wms">
     <getmap>
       <params>
         <FORMAT>image/JPEG</FORMAT>
         <LAYERS>MS</LAYERS>
         <MAP>/etc/geo/map.map</MAP>
       </params>
     </getmap>
     <http>
       <url>http://localhost/map/?</url>
     </http>
    </source>


    <format name="JPEG_FAST" type="JPEG">
     <compression>fast</compression>
     <colors>256</colors>
    </format>

    <tileset name="admin_countries">
     <source>admin_countries</source>
     <cache>disk</cache>
     <grid>WGS84</grid>
	<grid>g</grid>
     <format>JPEG_FAST</format>
     <metatile>5 5</metatile>
     <metabuffer>20</metabuffer>
     <expires>3600</expires>
    </tileset>

    <tileset name="zelda">
     <source>zelda</source>
     <cache>disk</cache>
     <grid>WGS84</grid>
	<grid>g</grid>
     <format>JPEG_FAST</format>
     <metatile>5 5</metatile>
     <metabuffer>20</metabuffer>
     <expires>3600</expires>
    </tileset>

    <default_format>JPEG_FAST</default_format>

    <service type="wms" enabled="true">
     <full_wms>assemble</full_wms>
     <resample_mode>bilinear</resample_mode>
     <format>JPEG_FAST</format>
     <maxsize>4096</maxsize>
    </service>
    <service type="wmts" enabled="true"/>
    <service type="tms" enabled="true"/>
    <service type="kml" enabled="true"/>
    <service type="gmaps" enabled="true"/>
    <service type="ve" enabled="true"/>
    <service type="demo" enabled="true"/>

    <errors>report</errors>
    <lock_dir>/tmp/</lock_dir>
    <log_level>debug</log_level>

</mapcache>
